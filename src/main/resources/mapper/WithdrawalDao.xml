<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.sys.dao.WithdrawalDao">

    <select id="findWithdrawalById" resultType="com.example.sys.entity.Withdrawal">
        SELECT createDate,money,state,xing,zfb,gomoney FROM `withdrawal` WHERE `aid`={aid}
    </select>

</mapper>